{
  "version": 2,
  "name": "reactjs-films-homework",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/movie/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/popular",
      "dest": "/index.html"
    },
    {
      "src": "/upcoming",
      "dest": "/index.html"
    },
    {
      "src": "/top_rated",
      "dest": "/index.html"
    },
    {
      "src": "/Genres",
      "dest": "/index.html"
    },
    {
      "src": "/Search",
      "dest": "/index.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}